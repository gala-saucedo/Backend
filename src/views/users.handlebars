<div>
    <h1>Bienvenido {{ user.full_name }} </h1>
    {{#if isAdmin}}
        {{#each users}}
            <div>
                <p>Nombre: {{this.full_name}} - email: {{this.email}}  </p>
                <hr>
            </div>
        {{/each}}
    {{else}}
        <h2>No tienes permiso para ver los usuarios</h2>
    {{/if}}
</div>
<script src="/static/assets/js/index.js"></script>